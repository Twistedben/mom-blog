<div class="">
  <h1 class="text-3xl font-bold">Keespakes</h1>
  
  <ul class="mt-4 border-2 border-gray-500 rounded">
    <% @articles.each do |article| %>
      <% unless article.archived? %>
        <li class="mb-2 font-semibold">
          <%= link_to article.title, article %>
          <p><%= article.author %></p>
        </li>
        </li>
      <% end %>
    <% end %>
  </ul>
</div>

